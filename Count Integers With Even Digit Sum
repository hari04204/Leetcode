class Solution {
    public int countEven(int num) {
        int count=0;
        for(int i = 1; i<=num; i++){
            if((digitSum(i)&1) != 1) count++;
        }
        return count;
    }
    public int digitSum(int num){
        int sum = 0 ;
        int temp = num;
        while(temp!=0){
            sum+= (temp%10);
            temp/=10;
        }
        return sum;
    }
}
